<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Search">

    <TextView
        android:id="@+id/textViewBirdName"
        android:layout_width="209dp"
        android:layout_height="0dp"
        android:layout_marginTop="54dp"
        android:hint="Bird Name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewHighImportance" />

    <TextView
        android:id="@+id/textViewZipCode"
        android:layout_width="209dp"
        android:layout_height="25dp"
        android:layout_marginTop="28dp"
        android:hint="Zip Code"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewBirdName" />

    <TextView
        android:id="@+id/textViewUserEmail"
        android:layout_width="209dp"
        android:layout_height="25dp"
        android:layout_marginTop="30dp"
        android:hint="Bird Finder Email"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewZipCode" />

    <TextView
        android:id="@+id/textViewImportanceScore"
        android:layout_width="209dp"
        android:layout_height="25dp"
        android:layout_marginTop="40dp"
        android:hint="Importance Score"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewUserEmail" />

    <TextView
        android:id="@+id/textViewHighImportance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="103dp"
        android:layout_marginLeft="103dp"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="97dp"
        android:layout_marginRight="97dp"
        android:text="Bird With the Highest Importance"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>